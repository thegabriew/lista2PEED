dic = {}

esc = int(input("Quantas chaves serão adicionadas?\n"))

def add():
    chave = input("Digite uma chave: ")
    valor = input("Digite um valor para a chave: ")
    dic[chave] = valor

for i in range (esc):
    add()

if 'profissão' in dic:
    print("A chave profissão está no dicionário.")

else:
    print("A chave profissão não está no dicionário.")
